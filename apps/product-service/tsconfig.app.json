{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "types": [
      "node",
      "express"
    ],
    "rootDir": "../../",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "tsBuildInfoFile": "dist/tsconfig.app.tsbuildinfo"
  },
  "include": [
    "src/**/*.ts",
    "../../packages/**/*.ts",
    "../../packages/error-handler/**/*.ts",
    "../../packages/libs/**/**/*.ts",
    "../../packages/middleware/**/*.ts"
  ],
  "exclude": []
}
